def main():
    a = 21
    b = a + 6
    c = b + 20
    d = c + a 
    e = c 

    print("Anton is", a)
    print("Beth is " + str(b))
    print("Chen is " + str(c))
    print("Drew is", d)
    print("Ethan is " + str(e))


# There is no need to edit code beyond this point

if __name__ == '__main__':
    main()
